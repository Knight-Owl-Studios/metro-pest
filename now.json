{
  "version": 2,
  "name": "metro-pest-website",
  "builds": [
    {
      "src": "web/package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "public"
      }
    },
    {
      "src": "api/email/index.js",
      "use": "@now/node"
    }
  ],
  "alias": [
    "metro-pest-website.now.sh"
  ],
  "routes": [
    {
      "src": "/api/email/?",
      "dest": "/api/email/index.js"
    },
    {
      "src": "/api/payments/?",
      "dest": "/api/payments.js"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}