{
  "version": 2,
  "name": "metro-pest-website",
  "builds": [
    {
      "src": "web/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "alias": ["metro-pest-website.now.sh"],
  "routes": [
    {
      "src": "/(.*)", "dest": "/web/$1"
    }
  ]
}
